{
    "name": "yangs1/lsf",
    "description": "Just a easy lumen swoole Framework.",
    "keywords": ["framework", "php", "laravel", "lumen", "swoole"],
    "license": "MIT",
    "type": "project",
    "require": {
        "illuminate/database": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/validation": "^5.5",
        "illuminate/pipeline": "^5.5",
        "monolog/monolog": "^1.23",
        "illuminate/events": "^5.5",
        "illuminate/cache": "^5.6",
        "illuminate/redis": "^5.5",
        "overtrue/laravel-filesystem-qiniu": "^1.0",
        "illuminate/encryption": "^5.6",
        "illuminate/hashing": "^5.6",
        "illuminate/bus": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Foundation\\": "foundation/"
        },
        "files": [
            "foundation/helpers.php"
        ],
        "classmap": [
            "foundation/Container/BoundMethod.php"
        ]
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master"
    }
}
